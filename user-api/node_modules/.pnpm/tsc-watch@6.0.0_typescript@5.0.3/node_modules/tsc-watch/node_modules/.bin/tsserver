#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yatech/Documents/ESGI/chalenge_semestriel/challenge-semestre2/user-api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/yatech/Documents/ESGI/chalenge_semestriel/challenge-semestre2/user-api/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@5.0.3/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../typescript@5.0.3/node_modules/typescript/bin/tsserver" "$@"
fi
